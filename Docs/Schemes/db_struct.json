for (u of t) {
	console.log("*** ****************************************")
	console.log("*** -> " + u.UID)
	console.log("--- -> my_settings")
	for (v of u.my_settings.vehicles) {
		console.log("--- -> vehicles")
		console.log("--- -> " + v.type)
		for (d of v.details) {
			console.log("--- ---> " + d.energy + " " + d.distance + " " + d.mesure + " " + d.capacity + " " + d.unit)
		}
	}

	console.log("--- -> activities")
	for (a of u.my_settings.activities) {
		console.log("--- -> " + a.activity)
		for (ti of a.time) {
			console.log("--- ---> " + ti)
		}
	}

	for (d of u.destinations) {
		console.log("--- -> " + d.name + " " + d.trip_date + " " + d.trip_time_minutes)
		var temps = 0
		for (e of d.steps) {
			console.log("--- ---> " + e.step + " " + e.start.name + " " + e.end.name + " " + e.trip_time_minutes)
			temps = temps + parseInt(e.trip_time_minutes)
			for (a of e.activities) {
				console.log("--- ------> " + a.activity + " " + a.name + " " + a.time_minutes)
				temps = temps + parseInt(a.time_minutes)
			}
		}
		console.log("--- -> temps du voyage " + temps + " minutes.")
	}
